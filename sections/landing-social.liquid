                                placeholder="Last Name"/>
                        </div>

                        <div class="email" 
                            :class="{'error': !email.valid}">

                            <div class="email-container">

                                <input v-model="email.address" 
                                    id="email"  
                                    type="email" 
                                    placeholder="Email"
                                    v-on:keyup.enter="addEmail()"
                                    @focus="resetEmail()"/>

                                <div class="submit_btn" 
                                    @click="addEmail()"> 
                                    Submit
                                </div>

                            </div>

                        </div>

                    </div>
                    <div v-if="email.sending" class="sending"> Sending... </div>
                </div>
            
            </div>
           
