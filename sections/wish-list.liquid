
{{ 'wishlist.css' | asset_url | stylesheet_tag }}

<div id="wish-list-drawer" class="wish-list on">

    <!-- start app -->

    
            <div id="wish-list-header" class="">
                <div class="close"> 
                    {% render 'icon-times-solid' %}
                </div>
                <h3>Wishlist</h3>

                <div class="account">
                   
                </div>

                <div class="sign-on">
                    <div class="e-mail-input">
                        <input id="emailAddress"type="text" v-bind="email.address" placeholder="Enter your e-mail" /> <button>Enter</button>
                    </div>
                    <p></p>
                </div>

                <div class="sending">
                    <div class="icon">{% render 'icon-spinner-third' %}</div> <div>Sending...</div>
                </div>

            </div>

            <ul class="product-list">
               
            </ul>


</div>

<script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
